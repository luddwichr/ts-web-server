{
  "name": "ts-web-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@types/body-parser": "^1",
    "@types/eslint": "^8",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "body-parser": "^1.20.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "start": "tsc && cd dist && node .",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.ts --ignore-path .prettierignore .",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@sinclair/typebox": "^0.25.20",
    "express": "^4.18.2"
  }
